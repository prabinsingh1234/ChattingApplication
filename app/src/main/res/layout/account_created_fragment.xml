<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    tools:context=".fragments.RAppRTCCityFragment">



    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="204dp"
        android:layout_height="122dp"
        android:layout_marginBottom="436dp"
        android:orientation="horizontal"
        android:padding="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.011"
        android:visibility="gone">

        <ImageView
            android:id="@+id/rectangle"
            android:layout_width="20dp"
            android:layout_height="7.7dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:id="@+id/oval"
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:id="@+id/oval1"
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:id="@+id/oval2"
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle_476" />

        <ImageView
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />

        <ImageView
            android:layout_width="7.7dp"
            android:layout_height="7.7dp"
            android:layout_marginStart="10.2dp"
            android:src="@drawable/rectangle" />
    </LinearLayout>

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="215dp"
        android:layout_height="215dp"
        android:layout_marginTop="122dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="fill_parent"
        android:layout_height="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="18dp"
        android:fontFamily="@font/circularstd_medium"
        android:text="@string/account_created"
        android:gravity="center_horizontal"
        android:textColor="#2e2d3d"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.175"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/videoView"
        app:layout_constraintVertical_bias="0.009" />


    <LinearLayout
        android:id="@+id/demo_button1"
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:layout_weight="1"
        android:layout_marginBottom="10dp"
        android:gravity="center|bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        app:layout_constraintVertical_bias="0.866"
        android:orientation="vertical" >

        <Button
            android:id="@+id/demo_button"
            style="?android:attr/buttonStyleSmall"
            android:background="@drawable/roundbutton"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:layout_gravity="center_horizontal|center"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:fontFamily="sans-serif-medium"
            android:text="Make Your First Call"
            />
        <ImageView
            android:id="@+id/oval2111"
            android:layout_width="66dp"
            android:layout_height="7.7dp"
            android:src="@drawable/rectangle_476"
            android:layout_marginStart="10.2dp" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>